{
    "name": "protonlabs/apns-php",
    "type": "library",
    "description": "Apple Push Notification & Feedback Provider",
    "keywords": ["APNS", "apple", "push", "notification"],
    "homepage": "https://github.com/ProtonMail/ApnsPHP",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=7.4",
        "psr/log": "^1 || ^2 || ^3",
        "lib-openssl": "*",
        "lcobucci/jwt": "^4.1.0",
        "ext-curl": "*"
    },
    "autoload": {
        "psr-4": {
            "ApnsPHP\\": "ApnsPHP"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.6"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
